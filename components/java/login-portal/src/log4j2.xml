<Configuration status="WARN">
  <Appenders>
	<Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout>
		  <Pattern>%d{dd-MMM-yyyy HH:mm:ss.SSS}{UTC} %level [%t] %logger %msg%xEx%n</Pattern>
      </PatternLayout>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="STDOUT"/>
    </Root>
  </Loggers>
</Configuration>
